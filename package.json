  "scripts": {
    "emulator": "C: && ( CD C:/Users/eCentrix/AppData/Local/Android/Sdk/emulator/ && emulator -avd Pixel_2_API_30 )",
    "kill": "adb -e emu kill",
    "android": "react-native run-android",
    "start": "react-native start --reset-cache",
